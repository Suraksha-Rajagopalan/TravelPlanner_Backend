{
  "runtime": "Net90",
  "documentGenerator": {
    "fromSwagger": {
      "url": "https://localhost:5276/swagger/v1/swagger.json"
    }
  },
  "codeGenerators": {
    "openApiToTypeScriptClient": {
      "template": "Angular",
      "typeScriptVersion": 4.7,
      "generateClientClasses": true,
      "generateOptionalParameters": true,
      "injectionTokenType": "InjectionToken",
      "output": "./tp-api-client-angular/index.ts"
    }
  }
}
